class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        """The key of this question is swapping values, two pointers"""
        slow = 0
        for fast in range(len(nums)):
            if nums[fast] != 0 and nums[slow] == 0:
                nums[slow], nums[fast] = nums[fast], nums[slow] # if it is zero, move everything to the end.
            if nums[slow]!= 0: # if not zero, move to the next pointer 
                slow += 1
                
        """Time Complexity: O(n): fast pointer moves through every single element inside the array. Space complexity: O(1): no additional operators added in the list"""
                
        
                
        
